{
  "name": "kv-reader",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/reader.mjs",
  "module": "src/reader.mjs",
  "description": "decode kv config files as used by subversion",
  "keywords": [
    "svn",
    "subversion",
    "kv",
    "config"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/reader.mjs --section=API",
    "lint": "documentation lint src/reader.mjs",
    "posttest": "markdown-doctest",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^7.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "semantic-release": "^16.0.2"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/kv-reader.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/kv-reader/issues"
  },
  "homepage": "https://github.com/arlac77/kv-reader#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}
