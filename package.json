{
  "name": "kv-reader",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/reader.js",
  "module": "src/reader.js",
  "description": "decode kv config files as used by subversion",
  "keywords": [
    "svn",
    "subversion",
    "kv",
    "config"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "docs": "documentation readme src/reader.js --section=API",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c tests/rollup.config.js",
    "precover": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/reader.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "line-input-stream": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.8",
    "cracks": "3.1.2",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.1.0",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.9.15",
    "travis-deploy-once": "^5.0.7",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=8.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/kv-reader.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/kv-reader/issues"
  },
  "homepage": "https://github.com/arlac77/kv-reader#readme",
  "release": {
    "verifyRelease": "cracks"
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
