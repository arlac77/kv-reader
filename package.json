{
  "name": "kv-reader",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/reader.mjs",
  "module": "src/reader.mjs",
  "description": "decode kv config files as used by subversion",
  "keywords": [
    "svn",
    "subversion",
    "kv",
    "config"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "docs": "documentation readme src/reader.mjs --section=API",
    "posttest": "markdown-doctest",
    "lint": "documentation lint src/reader.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.1",
    "documentation": "^9.1.1",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.2",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/kv-reader.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/kv-reader/issues"
  },
  "homepage": "https://github.com/arlac77/kv-reader#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  },
  "xrelease": {
    "verifyRelease": "cracks"
  }
}
